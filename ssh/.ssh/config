Host *
    addkeystoagent yes
# SSH configuration

Host github.com
    hostname github.com
    user git
    identityfile ~/.ssh/id_rsa_github
    identitiesonly yes

Host gitlab.com
    hostname gitlab.com
    user git
    identityfile ~/.ssh/id_rsa_gitlab
    identitiesonly yes

Host vultr.btcassessors.com
    hostname vultr.btcassessors.com
    user root
    identityfile ~/.ssh/id_rsa_btca_vultr
    identitiesonly yes
    port 22
